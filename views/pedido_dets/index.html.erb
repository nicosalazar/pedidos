<p id="notice"><%= notice %></p>

<h1>Pedido Dets</h1>

<table>
  <thead>
    <tr>
      <th>Num pedido</th>
      <th>Producto code</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pedido_dets.each do |pedido_det| %>
      <tr>
        <td><%= pedido_det.num_pedido %></td>
        <td><%= pedido_det.producto_code %></td>
        <td><%= pedido_det.qty %></td>
        <td><%= link_to 'Show', pedido_det %></td>
        <td><%= link_to 'Edit', edit_pedido_det_path(pedido_det) %></td>
        <td><%= link_to 'Destroy', pedido_det, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pedido Det', new_pedido_det_path %>
